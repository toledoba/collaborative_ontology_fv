    <!-- https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
    


    <!-- https://w3id.org/transmodel/facilities#accessedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#accessedBy">
        <rdfs:comment xml:lang="en">This property sets the relationship between Site and Entrance</rdfs:comment>
        <rdfs:label xml:lang="en">accessed By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#accessibilityAssessment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#accessibilityAssessment">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#AccessibilityAssessment"/>
        <rdfs:comment xml:lang="en">TODO: Determine whether this should be moved to another module if the concept AccessibilityAssessment is moved as well</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used to relate a site element, a facility, etc., with a specific accessibility assessment. In some of the UML models of Transmodel this property is name as characerisedBy</rdfs:comment>
        <rdfs:label xml:lang="en">accessibility Assessment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#accessibilityLimitation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#accessibilityLimitation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Facility"/>
        <rdfs:label xml:lang="en">accessibilityLimitation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#accommodationFacility -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#accommodationFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Accommodation"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">TODO: the range of this property may be further restricted to those SKOS concepts that also comply with skos:broader tmkosfacility:FacilityType/AccomodationFacilityType</rdfs:comment>
        <rdfs:comment xml:lang="en">This property restricts the type of facility to one specific for accommodation from the SKOS codelist for types of facilities, including, for example, values like seating, sleeper, couchette, special seating, reclining seats, standing, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">accommodation facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#accomodation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#accomodation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/journeys#Service"/>
        <rdfs:label xml:lang="en">accomodation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#activateEquipment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#activateEquipment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Passenger"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Equipment"/>
        <rdfs:label xml:lang="en">activateEquipment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#activationAssignment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#activationAssignment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:label xml:lang="en">activationAssignment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#availableAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#availableAt">
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#SiteFacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Site"/>
        <rdfs:comment xml:lang="en">TODO: Check whether this property is also applicable to other things beyond Site Facility Sets</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a Site Facility Set with a Site</rdfs:comment>
        <rdfs:label xml:lang="en">available At</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#boardingPermission -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#boardingPermission">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#OnboardStay"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">TODO: the range of this property may be further restricted to those SKOS concepts that also comply with skos:broader tmkosfacility:FacilityType/BoardingPermissionType</rdfs:comment>
        <rdfs:comment xml:lang="en">This property describes the types of boarding permissions allowed for an Onboard Stay from the SKOS codelist for types of facilities, including normal, early boarding, late alighting, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">boarding permission</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#checkConstraint -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#checkConstraint">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#SiteComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Constraint"/>
        <rdfs:label xml:lang="en">siteComponentCheckConstraint</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#comprises -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#comprises">
        <rdfs:comment xml:lang="en">General property for all types of containment and aggregation</rdfs:comment>
        <rdfs:label xml:lang="en">comprises</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#comprisesEquipment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#comprisesEquipment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#comprises"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#FacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Equipment"/>
        <rdfs:comment xml:lang="en">TODO: Check carefully whether the range should be Equipment or, instead, something more general like Facility, since the differentiation between Facility and Equipment is sometimes unclear</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used to aggregate other types of facilities from a Facility Set, normally focused on those that are user defined and not from the predefined list.
In the Transmodel UML model this is represented as TypeOfEquipment, and in the Netex implementation the term otherFacilities is used.</rdfs:comment>
        <rdfs:label xml:lang="en">comprises equipment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#comprisesFacility -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#comprisesFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#comprises"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#FacilitySet"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Facility"/>
        <rdfs:comment xml:lang="en">This property is used to aggregate all the facilities that a facility set contains
In Netex, this is expressed with the element TypeOfFacilityRef, which is used to refer to facilities, and in the Transmodel UML ontology this is simply expressed as &apos;facility&apos;</rdfs:comment>
        <rdfs:label xml:lang="en">comprises facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#concernedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#concernedBy">
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#FacilitySet"/>
        <rdfs:comment xml:lang="en">This property allows connecting any object with a Facility Set</rdfs:comment>
        <rdfs:label xml:lang="en">concerned By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#containing -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#containing">
        <rdfs:comment xml:lang="en">This property allows specifying that a Stop contains Quays</rdfs:comment>
        <rdfs:label xml:lang="en">containing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#couchetteFacility -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#couchetteFacility">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Accommodation"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">TODO: the range of this property may be further restricted to those SKOS concepts that also comply with skos:broader tmkosfacility:FacilityType/CouchetteFacilityType</rdfs:comment>
        <rdfs:comment xml:lang="en">This property restricts the type of facility to one specific for couchette from the SKOS codelist for types of facilities, including, for example, values like T2, T3, C1, C2, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">couchette facility</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#determining -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#determining">
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#AccessibilityAssessment"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#AccessibilityLimitation"/>
        <rdfs:comment xml:lang="en">This property sets the relationship between Accessibility Assessment and Accessibility Limitation</rdfs:comment>
        <rdfs:label xml:lang="en">determining</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#displayAssignment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#displayAssignment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#JourneyPattern"/>
        <rdfs:comment xml:lang="en">The assignment of one ScheduledStopPoint and one JourneyPattern to a PassengerInformationEquipment specifying that information on the ScheduledStopPoint and the JourneyPattern will be provided (e.g. displayed, printed)</rdfs:comment>
        <rdfs:label xml:lang="en">displayAssignment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#equipmentType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#equipmentType">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Equipment"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/EquipmentType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property is used to link an equipment with the types of equipments available in the SKOS concept scheme of types of equipments</rdfs:comment>
        <rdfs:label xml:lang="en">equipment Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#equippedWith -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#equippedWith">
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#EquipmentPlace"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Equipment"/>
        <rdfs:comment xml:lang="en">TODO: relate it to a more general property for containment and partOf</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used to define the equipments that are available in an equipment place</rdfs:comment>
        <rdfs:label xml:lang="en">equipped With</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#facilityRequirement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#facilityRequirement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Vehicle"/>
        <rdfs:range rdf:resource="https://w3id.org/transmodel/facilities#Facility"/>
        <rdfs:comment xml:lang="en">A classification of public transport Vehicles according to the facilities available on the Vehicle</rdfs:comment>
        <rdfs:label xml:lang="en">facilityRequirement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#facilitySet -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#facilitySet">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#OBJECTPROPERTIESTOREVIEW"/>
        <rdfs:comment xml:lang="en">Set of Facilities available for a ServiceJourney or a JourneyPart. The set may be available only for a specific VehicleType within the service (e.g. carriage equipped with low floor)</rdfs:comment>
        <rdfs:label xml:lang="en">facilitySet</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#facilityType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#facilityType">
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#Facility"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property is used to link a facility with the types of facilities available in the SKOS concept scheme of types of facilities</rdfs:comment>
        <rdfs:comment xml:lang="en">facility type</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/transmodel/facilities#fareClass -->

    <owl:ObjectProperty rdf:about="https://w3id.org/transmodel/facilities#fareClass">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/transmodel/facilities#facilityType"/>
        <rdfs:domain rdf:resource="https://w3id.org/transmodel/facilities#ServiceFacilitySet"/>
        <rdfs:range>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#inScheme"/>
                        <owl:hasValue rdf:resource="https://w3id.org/transmodel/kos/facilities/FacilityType"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:range>
